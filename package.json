  "keywords": [
    "nrf-asset-tracker",
    "iot",
    "web-app",
    "golioth",
    "spa",
    "react",
    "vite"
  ],
  "author": "Nordic Semiconductor ASA | nordicsemi.no",
  "license": "BSD-3-Clause",
  "dependencies": {
    "@amcharts/amcharts5": "5.0.21",
    "@nordicsemiconductor/rsrp-bar": "5.0.0",
    "bootstrap": "5.1.3",
    "classnames": "2.3.1",
    "date-fns": "2.28.0",
    "e118-iin-list": "1.7.0",
    "fast-deep-equal": "3.1.3",
    "feather-icons": "4.28.0",
    "fp-ts": "2.11.8",
    "iconoir": "1.0.0",
    "iconoir-react": "2.1.0",
    "inter-ui": "3.19.3",
    "intro.js": "4.3.0",
    "jose": "4.3.8",
    "mcc-mnc-list": "1.1.8",
