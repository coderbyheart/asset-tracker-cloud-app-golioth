{
  "name": "@nordicsemiconductor/asset-tracker-cloud-app-golioth",
  "version": "0.0.0-development",
  "description": "nRF Asset Tracker Web Application for Golioth",
  "type": "module",
  "scripts": {
    "start": "snowpack dev",
    "build": "snowpack build",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NordicSemiconductor/asset-tracker-cloud-app-golioth-js.git"
  },
  "bugs": {
    "url": "https://github.com/NordicSemiconductor/asset-tracker-cloud-app-golioth-js/issues"
  },
  "homepage": "https://github.com/NordicSemiconductor/asset-tracker-cloud-app-golioth-js#readme",
  "keywords": [
    "Cloud",
    "IoT",
    "nRF Asset Tracker",
    "Nordic Semiconductor"
  ],
  "author": "Nordic Semiconductor ASA | nordicsemi.no",
  "license": "LICENSE",
  "dependencies": {
    "bootstrap": "5.1.3",
    "jose": "4.3.7",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@nordicsemiconductor/asset-tracker-cloud-code-style": "9.2.30",
    "@snowpack/plugin-react-refresh": "2.5.0",
    "@snowpack/plugin-typescript": "1.2.1",
    "@types/react": "17.0.37",
    "@types/react-dom": "17.0.11",
    "eslint-config-react-app": "6.0.0",
    "snowpack": "3.8.8"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --ext .js,.ts,.jsx,.tsx"
    ],
    "*.{md,json,yaml,yml,html}": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=16",
    "npm": ">=8"
  },
  "release": {
    "branches": [
      "saga"
    ],
    "remoteTags": true,
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/github"
    ]
  },
  "browserslist": [
    ">3%",
    "last 1 version",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "prettier": "@nordicsemiconductor/asset-tracker-cloud-code-style/.prettierrc"
}
