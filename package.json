{
  "name": "@nordicsemiconductor/asset-tracker-cloud-app-golioth",
  "version": "0.0.0-development",
  "description": "nRF Asset Tracker Web Application for Golioth",
  "type": "module",
  "scripts": {
    "start": "vite",
    "build": "vite build --emptyOutDir",
    "preview": "vite preview",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NordicSemiconductor/asset-tracker-cloud-app-golioth-js.git"
  },
  "bugs": {
    "url": "https://github.com/NordicSemiconductor/asset-tracker-cloud-app-golioth-js/issues"
  },
  "homepage": "https://github.com/NordicSemiconductor/asset-tracker-cloud-app-golioth-js#readme",
  "keywords": [
    "Cloud",
    "IoT",
    "nRF Asset Tracker",
    "Nordic Semiconductor"
  ],
  "author": "Nordic Semiconductor ASA | nordicsemi.no",
  "license": "LICENSE",
  "dependencies": {
    "@amcharts/amcharts5": "5.0.18",
    "@nordicsemiconductor/rsrp-bar": "4.1.2",
    "bootstrap": "5.1.3",
    "date-fns": "2.27.0",
    "e118-iin-list": "1.7.0",
    "fast-deep-equal": "3.1.3",
    "fp-ts": "2.11.5",
    "inter-ui": "3.19.3",
    "jose": "4.3.7",
    "mcc-mnc-list": "1.1.7",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-leaflet": "3.2.2",
    "react-router-dom": "6.2.1",
    "twemoji": "13.1.0",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@nordicsemiconductor/asset-tracker-cloud-code-style": "9.2.36",
    "@types/jest": "27.0.3",
    "@types/leaflet": "1.7.6",
    "@types/react": "17.0.38",
    "@types/react-dom": "17.0.11",
    "@types/twemoji": "12.1.2",
    "@types/uuid": "8.3.3",
    "@vitejs/plugin-react": "1.1.3",
    "eslint-config-react-app": "7.0.0",
    "jest": "27.4.5",
    "ts-jest": "27.1.2",
    "vite": "2.7.6"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --ext .js,.ts,.jsx,.tsx"
    ],
    "*.{md,json,yaml,yml,html}": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=16",
    "npm": ">=8"
  },
  "release": {
    "branches": [
      "saga"
    ],
    "remoteTags": true,
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/github"
    ]
  },
  "browserslist": [
    ">3%",
    "last 1 version",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "prettier": "@nordicsemiconductor/asset-tracker-cloud-code-style/.prettierrc",
  "jest": {
    "preset": "ts-jest",
    "testRegex": ".+\\.spec\\.ts$"
  }
}
