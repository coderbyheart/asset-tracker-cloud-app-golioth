{
  "name": "@nordicsemiconductor/asset-tracker-cloud-app-golioth",
  "version": "0.0.0-development",
  "description": "The nRF Asset Tracker Web Application for Golioth is a reference single-page application (SPA) developed using TypeScript.",
  "type": "module",
  "scripts": {
    "start": "vite",
    "build": "vite build --emptyOutDir",
    "preview": "vite preview",
    "prepare": "husky install"
