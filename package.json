    "react-dom": "17.0.2",
    "react-leaflet": "3.2.5",
    "react-router-dom": "6.2.1",
    "rfdc": "1.3.0"
  },
  "devDependencies": {
    "@nordicsemiconductor/asset-tracker-cloud-code-style": "11.0.7",
    "@types/classnames": "2.3.1",
    "@types/feather-icons": "4.7.0",
    "@types/intro.js": "3.0.2",
    "@types/jest": "27.4.0",
    "@types/leaflet": "1.7.8",
    "@types/react": "17.0.38",
